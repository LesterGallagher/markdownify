{
  "name": "markdownify",
  "description": "Markdownify is a simple and effective markdown/text file viewer. You can open any file in the markdown format. Markdownify can also view HTML files.",
  "version": "0.2.6",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "license": "NOLICENSE",
  "author": "Sem Postma",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --publish onTagOrDraft",
    "electron": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "deploy": "gh-pages -d build",
    "predeploy": "npm run build",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "analyze": "source-map-explorer build/static/js/*.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "classnames": "^2.3.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^11.4.9",
    "electron-builder": "^22.11.7",
    "electron-is-dev": "^2.0.0",
    "gh-pages": "^2.0.1",
    "github-markdown-css": "^3.0.1",
    "office-ui-fabric-react": "^6.188.2",
    "pretty-bytes": "^5.2.0",
    "query-string": "^6.4.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.0.8",
    "wait-on": "^3.2.0"
  },
  "build": {
    "extends": null,
    "appId": "com.ESStudio.Markdownify",
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "target": [
        "portable",
        "nsis",
        "appx",
        "zip"
      ],
      "icon": "icon.png",
      "publish": [
        {
          "provider": "github",
          "repo": "markdownify",
          "owner": "LesterGallagher",
          "publishAutoUpdate": true
        }
      ],
      "fileAssociations": [
        {
          "ext": [
            "html",
            "htm",
            "xhtml",
            "dhtml",
            "phtml",
            "jhtml",
            "mhtml",
            "rhtml",
            "shtml",
            "zhtml"
          ],
          "name": "HTML",
          "role": "Viewer"
        },
        {
          "ext": [
            "markdown",
            "mdown",
            "mkdn",
            "md",
            "mkd",
            "mdwn",
            "mdtxt",
            "mdtext",
            "text",
            "Rmd"
          ],
          "name": "Markdown",
          "role": "Viewer"
        }
      ]
    },
    "appx": {
      "applicationId": "markdownify",
      "identityName": "32415ESStudio.Markdownify",
      "backgroundColor": "#ffffff",
      "displayName": "Markdownify",
      "publisher": "CN=BD184AA1-66C8-45BE-87E1-3209E1C18536",
      "publisherDisplayName": "ESStudio"
    },
    "files": [
      "**/*",
      "!dist",
      "!src"
    ]
  },
  "dependencies": {
    "@babel/core": "7.4.3",
    "@svgr/webpack": "4.1.0",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.7.1",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^8.0.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "2.1.1",
    "custom-electron-titlebar": "^3.0.8",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "electron-context-menu": "^0.12.1",
    "electron-log": "^3.0.5",
    "electron-updater": "^4.0.6",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "file-loader": "3.0.1",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "jest": "24.7.1",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.7.1",
    "jest-watch-typeahead": "0.3.0",
    "mime-types": "^2.1.31",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "react-app-polyfill": "^1.0.0",
    "react-dev-utils": "^9.0.0",
    "resolve": "1.10.0",
    "rich-markdown-editor": "^11.13.0",
    "sass-loader": "7.1.0",
    "semver": "6.0.0",
    "style-loader": "0.23.1",
    "styled-components": "^5.3.0",
    "terser-webpack-plugin": "1.2.3",
    "turndown": "^5.0.3",
    "url-loader": "1.1.2",
    "webpack": "5.94.0",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
